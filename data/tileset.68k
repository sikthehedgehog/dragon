;****************************************************************************
; GfxTileset
; Tileset graphics.
;****************************************************************************

GfxTileset:
    incbin  "data/tileset/tileset.slz"
    even

;****************************************************************************
; Graphics for the various animations
;****************************************************************************

    incgfx  GfxCandle, "data/tileset/candle.4bpp"
    incgfx  GfxChain, "data/tileset/chain.4bpp"

;****************************************************************************
